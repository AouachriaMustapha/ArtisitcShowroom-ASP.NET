@model Domain.Entity.message

@{
    ViewBag.Title = "Delete";
Layout = "~/Views/Shared/_LayoutBack.cshtml";

}
<h2>Delete</h2>

<h3>Are you sure you want to delete this?</h3>
<div>
    <h4>Message</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.Label("Date Of send", htmlAttributes: new { @class = "control-label col-md-2" })
        </dt>

        <dd>
            @Html.DisplayFor(model => model.date)
        </dd>

        <dt>
            @Html.Label("Text Of Message", htmlAttributes: new { @class = "control-label col-md-2" })
        </dt>

        <dd>
            @Html.DisplayFor(model => model.text)
        </dd>

        


    </dl>

    @using (Html.BeginForm()) {
    @Html.AntiForgeryToken()

    <div class="form-actions no-color">
        <input type="submit" value="Delete" class="btn btn-default" /> |

    </div>
    }
</div>
<div>
    @Html.ActionLink("Back to List", "ListMessage")
</div>